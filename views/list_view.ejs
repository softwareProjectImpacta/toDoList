<!-- Listar as tarefas cadastradas -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lista de Tarefas</title>
</head>
<body>
    <ul>
        <% tasks.forEach(function(task) { %>
            <li><%= task.id_tarefa %> - <%= task.nm_tarefa %> - <%= task.id_usuario %></li>
        <% }); %>
    </ul>
</body>
</html>
